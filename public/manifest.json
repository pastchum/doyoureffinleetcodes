{
    "manifest_version": 3,
    "name": "DoYourEffinLeetcodes",
    "version": "1.0.0",
    "description": "Reminder for you guys to do your effin leetcodes",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
        "identity",
        "tabs",
        "storage",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules1.json"
      }
    ],
    "default_rule_resources": ["ruleset_1"]
    }
  }